{
	"name": "astra",
	"version": "2.0.0",
	"description": "Discord bot that plays music (WIP) and keeps up with game details.",
	"main": "dist/Astra.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"start": "tsc && node ./dist/Astra.js",
		"dev": "tsc && nodemon ./dist/Astra.js",
		"pm2": "pm2 start Astra --watch",
		"pm2-restart": "tsc && pm2 restart Astra"
	},
	"keywords": [
		"typescript",
		"sern",
		"discord.js",
		"music",
		"gaming",
		"api",
		"xp",
		"rank",
		"level"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Peter-MJ-Parker/astra.git"
	},
	"license": "UNLICENSED",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@google-cloud/aiplatform": "^3.0.0",
		"@google-cloud/automl": "^4.0.0",
		"@google-cloud/translate": "^8.0.1",
		"@sern/handler": "^3.0.2",
		"@spark.ts/logger": "^1.4.0",
		"axios": "^1.4.0",
		"canvas": "^2.11.2",
		"colorette": "^2.0.20",
		"discord-giveaways": "^6.0.1",
		"discord-html-transcripts": "^3.1.5",
		"discord.js": "^14.12.1",
		"glob": "^10.3.3",
		"googleapis": "^125.0.0",
		"https": "^1.0.0",
		"lavacord": "^1.1.9",
		"mongoose": "^7.4.3",
		"undici": "^5.23.0",
		"unfurl.js": "^6.3.2"
	},
	"overrides": {
		"discord-api-types": "0.37.20"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/node": "^20.5.0",
		"@types/node-fetch": "^2.6.4",
		"nodemon": "^3.0.1",
		"typescript": "^5.1.6"
	},
	"imports": {
		"#client": [
			"./dist/Structures/client/index.js"
		],
		"#utils": [
			"./dist/Structures/client/utils.js"
		],
		"#Astra": [
			"./dist/Astra.js"
		],
		"#logger": [
			"./dist/Structures/handler/sparky.js"
		],
		"#handler": [
			"./dist/Structures/handler/index.js"
		],
		"#plugins": [
			"./dist/Structures/handler/plugins.js"
		],
		"#schemas/*": [
			"./dist/Structures/mongo/schemas/*.js"
		]
	}
}
